{
    "name": "stepanenko3/laravel-runtime-config",
    "description": "Runtime config based on collection for Laravel",
    "keywords": [
        "laravel",
        "config",
        "store"
    ],
    "license": "MIT",
    "require": {
        "php": ">=8.0",
        "laravel/framework": ">=9.0"
    },
    "autoload": {
        "psr-4": {
            "Stepanenko3\\LaravelRuntimeConfig\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Stepanenko3\\LaravelRuntimeConfig\\RuntimeConfigServiceProvider"
            ],
            "aliases": {
                "RuntimeConfig": "Stepanenko3\\LaravelRuntimeConfig\\Facades\\RuntimeConfig"
            }
        }
    }
}
